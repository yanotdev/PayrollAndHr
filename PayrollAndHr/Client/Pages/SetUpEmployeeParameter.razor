@page "/setupemployeeparameter/"
@layout PayrollAndHr.Client.Shared.Layouts.SetUpLayout

<style>
    .spacing {
        margin-top: 5px;
    }

    .txt {
        min-width: 250px;
    }
</style>
<div class="row">
    @*Staff Title Start from here*@
    <div class="col-md-4">
        <div class="card">
            <div class="card-header"><strong>Staff Titles</strong></div>
            <div class="card-body">
                <div class="row">
                    <div class="col-md-3">
                    </div>
                    <div class="col-md-9">
                        <button class="btn btn-primary" type="submit" id="NewTitle"><i class="fa fa-file-o"></i> New</button>
                        <button class="btn btn-primary" type="submit" id="SaveTitle"><i class="fa fa-save"></i> Save</button>
                    </div>
                </div>
                <form method="post" class="form-inline">            
                
                    <div class="row">
                        <div class="col-md-3 spacing"><label class="form-control-label">Code:</label></div>
                        <div class="col-md-9 spacing">
                            <input type="text" name="txtTitleCode" class = "form-control txt" id = "txtTitleCode" disabled />                            
                        </div>
                        <div class="col-md-3 spacing">
                            <label class="form-control-label">Title:</label>
                        </div>
                        <div class="col-md-9 spacing">
                            <input type="text" name="txtTitle" class = "form-control txt" id = "txtTitle" Placeholder = "Type Title"/>                            
                        </div>
                    </div>

                </form>
                <br />
                <table class="table table-bordered table-hover table-custom-border" id="titletable">
                    <thead>
                        <tr>
                            <th>S/N</th>
                            <th>TITLE</th>
                            <th>CODE</th>
                            <th>ACTION</th>
                        </tr>
                    </thead>
                    <tbody id="titlebody">
                    </tbody>
                    <tfoot>
                    </tfoot>
                </table>
            </div>
        </div>
    </div>
    @*Employee Level Start from here*@
    <div class="col-md-4">
        <div class="card">
            <div class="card-header"><strong>Employee Levels</strong></div>
            <div class="card-body">
                <div class="row">
                    <div class="col-md-3">
                    </div>
                    <div class="col-md-9">
                        <button class="btn btn-primary" type="submit" id="NewLevel"><i class="fa fa-file-o"></i> New</button>
                        <button class="btn btn-primary" type="submit" id="SaveLevel"><i class="fa fa-save"></i> Save</button>
                    </div>
                </div>
                <form method="post" class = "form-inline" >
              
                    <div class="row">
                        <div class="col-md-3 spacing"><label class="form-control-label">Code:</label></div>
                        <div class="col-md-9 spacing">
                            <input type="text" name="txtLevelCode" class = "form-control txt" id = "txtLevelCode" disabled/>                            
                        </div>
                        <div class="col-md-3 spacing">
                            <label class="form-control-label">Level:</label>
                        </div>
                        <div class="col-md-9 spacing">
                            <input type="text" name="txtLevel" class = "form-control txt" id = "txtLevel" Placeholder = "Type Level"/>                  
                        </div>
                    </div>
                </form>
                <br />
                <table class="table table-bordered table-hover table-custom-border" id="leveltable">
                    <thead>
                        <tr>
                            <th>S/N</th>
                            <th>LEVEL</th>
                            <th>CODE</th>
                            <th>ACTION</th>
                        </tr>
                    </thead>
                    <tbody id="levelbody"></tbody>
                    <tfoot>
                    </tfoot>
                </table>
            </div>
        </div>
    </div>
    @*Employment Type Start from here*@
    <div class="col-md-4">
        <div class="card">
            <div class="card-header"><strong>Employment Types</strong></div>
            <div class="card-body">
                <div class="row">
                    <div class="col-md-3">
                    </div>
                    <div class="col-md-9">
                        <button class="btn btn-primary" type="submit" id="NewEmpTypeCode"><i class="fa fa-file-o"></i> New</button>
                        <button class="btn btn-primary" type="submit" id="SaveempType"><i class="fa fa-save"></i> Save</button>
                    </div>
                </div>
                <form method="post" class="form-inline">               
                    <div class="row">
                        <div class="col-md-3 spacing"><label class="form-control-label">Code:</label></div>
                        <div class="col-md-9 spacing">
                            <input type="text" name="txtempCode" class = "form-control txt" id = "txtempCode" disabled/>     
                        </div>
                        <div class="col-md-3 spacing">
                            <label class="form-control-label">Description:</label>
                        </div>
                        <div class="col-md-9 spacing">
                            <input type="text" name="txtempType" class = "form-control txt" id = "txtempType" Placeholder = "Type Description"/>                            
                        </div>
                    </div>
                </form>
                <br />
                <table class="table table-bordered table-hover table-custom-border" id="empTypetable">
                    <thead>
                        <tr>
                            <th>S/N</th>
                            <th>DESCRIPTION</th>
                            <th>CODE</th>
                            <th>ACTION</th>
                        </tr>
                    </thead>
                    <tbody id="empTypebody"></tbody>
                    <tfoot>
                    </tfoot>
                </table>
            </div>
        </div>
    </div>
</div>
<br />
<div class="row">
    @*Staff Dept start from here*@
    <div class="col-md-4">
        <div class="card">
            <div class="card-header"><strong>Staff Department</strong></div>
            <div class="card-body">
                <div class="row">
                    <div class="col-md-3">
                    </div>
                    <div class="col-md-9">
                        <button class="btn btn-primary" type="submit" id="NewDept"><i class="fa fa-file-o"></i> New</button>
                        <button class="btn btn-primary" type="submit" id="SaveDept"><i class="fa fa-save"></i> Save</button>
                    </div>
                </div>
                <form method="post" class="form-inline">
                
                    <div class="row">
                        <div class="col-md-3 spacing"><label class="form-control-label">Code:</label></div>
                        <div class="col-md-9 spacing">
                            <input type="text" name="txtDeptCode" class = "form-control txt" id = "txtDeptCode" disabled/>
                           
                        </div>
                        <div class="col-md-3 spacing">
                            <label class="form-control-label">Department:</label>
                        </div>
                        <div class="col-md-9 spacing">
                            <input type="text" name="txtdept" class = "form-control txt" id = "txtdept" Placeholder = "Type Department" />
                            
                        </div>
                    </div>
                </form>

                
                <br />
                <table class="table table-bordered table-hover table-custom-border" id="deptTable">
                    <thead>
                        <tr>
                            <th>S/N</th>
                            <th>DEPT</th>
                            <th>CODE</th>
                            <th>ACTION</th>
                        </tr>
                    </thead>
                    <tbody id="deptbody"></tbody>
                    <tfoot>
                    </tfoot>
                </table>
            </div>
        </div>
    </div>
    @*Staff Designation Start from here*@
    <div class="col-md-4">
        <div class="card">
            <div class="card-header"><strong>Staff Designation</strong></div>
            <div class="card-body">
                <div class="row">
                    <div class="col-md-3">
                    </div>
                    <div class="col-md-9">
                        <button class="btn btn-primary" type="submit" id="NewDes"><i class="fa fa-file-o"></i> New</button>
                        <button class="btn btn-primary" type="submit" id="SaveDes"><i class="fa fa-save"></i> Save</button>
                    </div>
                </div>
                <form method="post" class="form-inline">
                
                    <div class="row">
                        <div class="col-md-3 spacing"><label class="form-control-label">Code:</label></div>
                        <div class="col-md-9 spacing">
                            <input type="text" name="txtDesCode" class = "form-control txt" id = "txtDesCode" disabled/>
                            
                        </div>
                        <div class="col-md-3 spacing">
                            <label class="form-control-label">Designation:</label>
                        </div>
                        <div class="col-md-9 spacing">
                            <input type="text" name="txtDes" class = "form-control txt" id = "txtDes" Placeholder = "Type Designation"/>
                            
                        </div>
                    </div>
                    </form>

                
                <br />
                <table class="table table-bordered table-hover table-custom-border" id="DesTable">
                    <thead>
                        <tr>
                            <th>S/N</th>
                            <th>DESIGNATION</th>
                            <th>CODE</th>
                            <th>ACTION</th>
                        </tr>
                    </thead>
                    <tbody id="descbody"></tbody>
                    <tfoot>
                    </tfoot>
                </table>
            </div>
        </div>
    </div>

    @*Start the Academic Degree*@
    <div class="col-md-4">
        <div class="card">
            <div class="card-header"><strong>Academic Degree</strong></div>
            <div class="card-body">
                <div class="row">
                    <div class="col-md-3">
                    </div>
                    <div class="col-md-9">
                        <button class="btn btn-primary" type="submit" id="NewDeg"><i class="fa fa-file-o"></i> New</button>
                        <button class="btn btn-primary" type="submit" id="SaveDeg"><i class="fa fa-save"></i> Save</button>
                    </div>
                </div>
                <form method="post" class="form-inline">
               <div class="row">
                        <div class="col-md-3 spacing"><label class="form-control-label">Code:</label></div>
                        <div class="col-md-9 spacing">
                            <input type="text" name="txtDegCode" class = "form-control txt" id = "txtDegCode" disabled/>
                            
                        </div>
                        <div class="col-md-3 spacing">
                            <label class="form-control-label">Designation:</label>
                        </div>
                        <div class="col-md-9 spacing">
                            <input type="text" name="txtDeg" class = "form-control txt" id = "txtDeg" Placeholder = "Type Degree"/>
                           
                        </div>
                    </div>


                </form>
                <br />
                <table class="table table-bordered table-hover table-custom-border" id="DesTable">
                    <thead>
                        <tr>
                            <th>S/N</th>
                            <th>DESIGNATION</th>
                            <th>CODE</th>
                            <th>ACTION</th>
                        </tr>
                    </thead>
                    <tbody id="degbody"></tbody>
                    <tfoot>
                    </tfoot>
                </table>
            </div>
        </div>
    </div>

</div>
<br />

